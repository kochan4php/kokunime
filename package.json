{
  "name": "kokunime",
  "version": "0.1.0",
  "private": true,
  "author": "Deo Subarno",
  "scripts": {
    "update-deps": "pnpm update --latest",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "docker:up": "pnpm docker:down && docker compose up -d",
    "docker:down": "docker compose down",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cheerio": "1.0.0-rc.12",
    "husky": "^9.0.11",
    "next": "14.2.4",
    "nextjs-toploader": "^1.6.12",
    "npmlog": "^7.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "sharp": "^0.33.4"
  },
  "devDependencies": {
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "9.5.0",
    "eslint-config-next": "14.2.4",
    "lint-staged": "^15.2.7",
    "postcss": "8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "3.4.4",
    "typescript": "5.5.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm prettier:fix"
    ]
  }
}
