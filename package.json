{
  "name": "kokunime",
  "version": "0.1.0",
  "private": true,
  "author": "Deo Subarno",
  "scripts": {
    "update-deps": "pnpm update --latest",
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "prettier:check": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "docker:up": "pnpm docker:down && docker compose up -d",
    "docker:down": "docker compose down",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "cheerio": "1.0.0-rc.12",
    "husky": "^9.0.11",
    "next": "14.2.1",
    "nextjs-toploader": "^1.6.11",
    "npmlog": "^7.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.33.3"
  },
  "devDependencies": {
    "@types/node": "20.12.7",
    "@types/react": "18.2.79",
    "@types/react-dom": "18.2.25",
    "autoprefixer": "10.4.19",
    "eslint": "9.0.0",
    "eslint-config-next": "14.2.1",
    "lint-staged": "^15.2.2",
    "postcss": "8.4.38",
    "prettier": "^3.2.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "pnpm prettier:fix"
    ]
  }
}
