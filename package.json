{
    "name": "kokunime",
    "version": "0.1.0",
    "private": true,
    "author": "Deo Subarno",
    "scripts": {
        "update-deps": "pnpm update --latest",
        "dev": "next dev --turbo",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "prettier:check": "prettier . --check",
        "prettier:fix": "prettier . --write",
        "docker:up": "pnpm docker:down && docker compose up -d",
        "docker:down": "docker compose down",
        "prepare": "husky install"
    },
    "dependencies": {
        "axios": "^1.6.0",
        "husky": "^8.0.3",
        "next": "14.0.1",
        "nextjs-toploader": "^1.5.3",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "sharp": "^0.32.6"
    },
    "devDependencies": {
        "@types/node": "20.8.10",
        "@types/react": "18.2.33",
        "@types/react-dom": "18.2.14",
        "autoprefixer": "10.4.16",
        "eslint": "8.52.0",
        "eslint-config-next": "14.0.1",
        "lint-staged": "^15.0.2",
        "postcss": "8.4.31",
        "prettier": "^3.0.3",
        "tailwindcss": "3.3.5",
        "typescript": "5.2.2"
    },
    "lint-staged": {
        "**/*.{js,jsx,ts,tsx}": [
            "pnpm prettier:fix"
        ]
    }
}
